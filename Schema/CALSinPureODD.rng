                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="colspec">
      <element name="colspec" ns="http://www.oasis-open.org/specs/tm9901">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Specifies a column, a vertical portion of a table.</a:documentation>
         <empty/>
         <optional>
            <attribute name="colnum">
